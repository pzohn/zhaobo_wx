/***********navbar***********/
.navbar{
	height:115rpx;
	width:100%;
	position:fixed;
	bottom:0rpx;
	z-index:999;
	background:#fff;
	display:flex;
	justify-content:center;
	border-top:solid 1px #dbdbdb;
	align-items:center;
}
.navbar_item{

	width:25%;
	text-align:center;

}
.navbar .text{
	color:#656565;
}
.navbar .this-text{
	color:#21c3d4;
}

.navigator-hover {
	background:none;
	color:#21c3d4;
}



/**index.wxss**/
.input_note{width:23%;
	color:#363636;
	font-size:32rpx;
}
.users_detail{
	width:100%;
	align-items:center;
}
.user_input{
	height:90rpx;
	line-height:90rpx;
	border-bottom:solid 1px #e0e0e0;
	padding:0px 25rpx;
	align-items:center;
	}
.user_input input{
	height:32rpx;
	line-height:32rpx;
	color:#000000;
	font-size:30rpx;
	width:77%;
}
.user_input image{
	width:11rpx;
	height:20rpx;
}
.address_right text{color:#b2b2b2;font-size:28rpx;}
.address_right image{margin-left:15rpx;}
.address_set{width:200rpx;}
.submit_info{
	position:absolute;
	bottom:280rpx;
}